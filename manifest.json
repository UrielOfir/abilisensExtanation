{
    "name": "Abilisense Extantion",
    "version": "1.0",
    "description": "Listening to the sounds that the microphone picks up, identifying unusual sounds and sending them to abilisense",
    "permissions": ["declarativeContent"],
    "background": {
      "scripts": ["recorderjs/WebAudioRecorder.js", "background.js"],
      "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/abiIcon.png",
        "32": "images/abiIcon.png",
        "48": "images/abiIcon.png",
        "128": "images/abiIcon.png"
      }

    },
    "icons": {
      "16": "images/abiIcon.png",
      "32": "images/abiIcon.png",
        "48": "images/abiIcon.png",
        "128": "images/abiIcon.png"
    },
    "manifest_version": 2
  }
  