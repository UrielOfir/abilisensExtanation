{
    "name": "Abilisense Extantion",
    "version": "1.0",
    "description": "Listening to the sounds that the microphone picks up, identifying unusual sounds and sending them to abilisense",
    "permissions": ["declarativeContent", "storage", "filesystem"],
    "background": {
      "scripts": ["WavAudioEncoder.js", "WebAudioRecorder.js", "background.js" ],
      "persistent": false
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/abiIcon.png",
        "32": "images/abiIcon.png",
        "48": "images/abiIcon.png",
        "128": "images/abiIcon.png"
      }
    
    },
    "icons": {
      "16": "images/abiIcon.png",
      "32": "images/abiIcon.png",
        "48": "images/abiIcon.png",
        "128": "images/abiIcon.png"
    },
    "requires": true,
    "lockfileVersion": 1,
    "dependencies": {
      "audiobuffer-to-wav": {
        "version": "1.0.0",
        "resolved": "https://registry.npmjs.org/audiobuffer-to-wav/-/audiobuffer-to-wav-1.0.0.tgz",
        "integrity": "sha1-1bQyJxRV5/7laxEc0PjWINf54QU="
      }
    },
    "manifest_version": 2
  }
  